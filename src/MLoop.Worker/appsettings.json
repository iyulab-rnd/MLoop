{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "MLoop.Worker": "Debug"
    },
    "Console": {
      "FormatterName": "json",
      "FormatterOptions": {
        "IncludeScopes": true,
        "TimestampFormat": "yyyy-MM-dd HH:mm:ss",
        "UseUtcTimestamp": true
      }
    }
  },
  "Storage": {
    "BasePath": "/var/data/mloop"
  },
  "Worker": {
    "WorkerId": null,
    "IdleTimeout": "00:10:00",
    "JobPollingInterval": "00:00:05",
    "JobTimeout": "08:00:00"
  },
  "MLNet": {
    "TrainPath": "mlnet",
    "PredictPath": "mlnet-predict"
  },
  "ShutdownTimeout": 30
}