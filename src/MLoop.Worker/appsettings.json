{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Storage": {
    "BasePath": "/var/data/mloop"
  },
  "Worker": {
    "WorkerId": null,
    "IdleTimeout": "00:10:00", // 10분
    "JobPollingInterval": "00:00:05", // 5초
    "JobTimeout": "08:00:00" // 8시간
  },
  "ShutdownTimeout": 30 // 30초
}